/* VARIABLE */
/* FONT */
@font-face {
  font-family: 'value-regular';
  src: url({{ 'value-sans-regular-pro.woff2' | asset_url }}) format('woff2'),
    url({{ 'value-sans-regular-pro.woff' | asset_url }}) format('woff');
  font-weight: normal;
  font-style: normal;
  font-display: swap;
}

@font-face {
  font-family: 'value-bold';
  src: url({{ 'value-sans-bold-pro.woff2' | asset_url }}) format('woff2'),
    url({{ 'value-sans-bold-pro.woff' | asset_url }}) format('woff');
  font-weight: normal;
  font-style: normal;
  font-display: swap;
}

@font-face {
  font-family: 'value-medium';
  src: url({{ 'value-sans-medium.woff2' | asset_url }}) format('woff2'),
    url({{ 'value-sans-medium.woff' | asset_url }}) format('woff');
  font-weight: normal;
  font-style: normal;
  font-display: swap;
}

$fnt_regular: "value-medium",sans-serif;
$fnt_signature: "golden-font",sans-serif;
$hover_main: #89baf5;
/* GLOBAL */
body,
input,
textarea,
button,
select,
.disclosure__toggle {
  letter-spacing: 0;
}

h1, .h1, h2, .h2, h3, .h3, h4, .h4, h5, .h5, h6, .h6 {
  font-family: value-medium,sans-serif;
  letter-spacing: 0;
  text-transform: unset;
  line-height: 1;
  font-weight: normal;
}

h3, .h3, h5, .h5 {
  -webkit-font-smoothing: subpixel-antialiased;
}

.site-footer .h4 {
  -webkit-font-smoothing: subpixel-antialiased;
}

@media(max-width: 767px) {
  h2, .h2 {
    font-family: 'value-bold';
  }
}

h2,
.h2,
h2 p {
  font-size: 40px;
  line-height: 45px;
  @media(max-width: 767px) {
    font-size: 24px;
    line-height: 30px;
  }
}
