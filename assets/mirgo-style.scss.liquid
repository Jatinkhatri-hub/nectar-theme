  padding-bottom: 1px !important;
  line-height: 46px !important;
  background-color: #F65636 !important;
  border-color: #F65636 !important;
}

.instagram-photo img {
  max-width: 235px;
  @media (max-width: 767px) {
    max-width: 100%;
  }
}

.btn-navy {
  border-color: #142444;
  color: #fff;
  line-height: 48px;
  @media (max-width: 767px) {
