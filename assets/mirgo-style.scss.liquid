input {
  font-size: 17px;
}

.btn,
.shopify-payment-button
.shopify-payment-button__button--unbranded {
  min-height: 50px !important;
  border-radius: 30px !important;
  padding-left: 30px !important;
  padding-right: 30px !important;
  font-family: $fnt_regular;
  font-weight: normal;
  line-height: 48px;
  padding-top: 0px;
  padding-bottom: 0px;
  &:hover {
    transition: 0.5s;
  }
  @media(max-width: 767px) {
    min-height: 40px !important;
    line-height: 40px;
  }
}
.btn {
  &.btn-white {
    border-color: #fff;
    color: #fff;
    background: transparent;
    background-color: transparent;
    &:hover {
      background-color: #89baf5;
      border-color: #89baf5;
      transition: 0.5s;
    }
  }
  &.non-button-recharge {
    &.hidden {
      display: none;
    }
  }
  &.product-form__cart-submit {
    &.hidden {
      display: none;
    }
    width: auto;
    text-transform: capitalize !important;
    padding-left: 30px !important;
    font-weight: 100;
    padding-right: 30px !important;
    background-color: #142444;
    border-color: #142444;
    color: #fff;
    transition: 0.5s;
    margin-bottom: 0;
    padding-top: 0px;
    line-height: 48px;
    padding-bottom: 0px;
    width: 100%;
    &:hover {
      background-color: #F65636;
      border-color: #F65636;
    }
    span {
      font-size: 16px;
      letter-spacing: 0.1rem;
      font-family: "value-regular";
    }
  }
}

#product_single_main .swiper-pagination-bullet-active {
  background: #142444 !important;
}

.product-form__cart-submit-orange {
  width: auto;
  text-transform: capitalize !important;
  padding-left: 30px !important;
  font-weight: 100;
  padding-right: 30px !important;
  background-color: #F65636;
  border-color: #F65636;
  color: #fff;
  transition: 0.5s;
  margin-bottom: 0;
  padding-top: 0px;
  line-height: 48px;
  padding-bottom: 0px;
  width: 312px;
  &:hover {
    background-color: #F65636 !important;
    border-color: #F65636 !important;
  }
  span {
    font-size: 16px;
    letter-spacing: 0.1rem;
    font-family: "value-regular";
