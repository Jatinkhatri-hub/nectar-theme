/* Custom Icon list */
.link_underline {
  text-transform: capitalize;
  padding-bottom: 10px;
  color: #142444;
  font-family: $fnt_regular;
  -webkit-font-smoothing: subpixel-antialiased;
  display: inline-block;
  line-height: 1;
  position: relative;
  letter-spacing: 2px;
  @media(max-width: 767px) {
    font-size: 16px;
    letter-spacing: 1px;
  }
  &:after {
    content: "";
    width: 100%;
    bottom: 0;
    left: 0;
    position: absolute;
    height: 2px;
    background: #142444;
  }
  &:before {
    content: "";
    width: 0;
    bottom: 0;
    left: 0;
    position: absolute;
    height: 2px;
    background: #294376;
    transition: 1s;
  }
  &:hover {
    color: #142444 !important;
    &:after {
      background: #142444;
    }
    /*
    &:before {
    width: 100%;
    transition: 1s;
  }
    */
  }
}
.cross  {
  li {
    position: relative;
    padding-left: 30px;

    /*
    display: flex;
    align-items: center;
    */
    &:before {
      content: '';
      height: 16px;
      border-left: 2px solid #f65636;
      position: absolute;
      transform: rotate(-45deg);
      left: 10px;
      top: 6px;
    }
    &:after {
      content: '';
      height: 16px;
      border-left: 2px solid #f65636;
      position: absolute;
      transform: rotate(45deg);
      left: 10px;
      top: 6px;
    }
  }
}


.check {
  li {
    position: relative;
    padding-left: 30px;

    /*
    display: flex;
    align-items: center;
    */
    &:before {
      content: '';
      display: block;
      width: 10px;
      height: 10px;
      background-color: #f65636;
      border-radius: 50%;
      position: absolute;
      left: 8px;
      top: 6px;
    }
  }
}

.checkmark-orange {
  li {
    position: relative;
    padding-left: 30px;

    /*
    display: flex;
    align-items: center;
    */
    &:before {
      content: '';
      display: block;
      width: 6px;
      height: 15px;
      border: solid #f65636;
      border-width: 0 2px 2px 0;
      transform: rotate(45deg);
      position: absolute;
      left: 8px;
      top: 3px;
    }
  }
}


