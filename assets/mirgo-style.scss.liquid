.mirgo-animation-blocks {
  position: relative;
  .path {
    fill: none;
    stroke: #cfe4fb;
    stroke-width: 120px;

  }
  @keyframes move {
    100% {
      stroke-dashoffset: 0;
    }
  }
  .data {
    height: 100%;
    position: absolute;
    z-index: -1;
    top:-160px;
    width: 100%;
    svg {
      overflow: unset;
    }
    &.data-content-one {
      height: unset;
    }
  }
  .section-container {
    .page-width {
      .section-content {
        .grid-blocks {
          .grid-items--section {
            margin-bottom: 85px;
            @media(max-width: 900px ){
              margin-bottom: 20px;
            }
            .grid-inner-items {
              display: flex;
              flex-wrap: wrap;

              @media(min-width: 901px ){
                margin-left: -8.33333%;
              }

              .grid-items-columns {
                max-width: 50%;
                width: 100%;
                display: flex;
                align-items: center;
                &.grid-img-cols {
                  @media(min-width: 901px ){
                    max-width: 50%;
                  }
                }
                @media(max-width: 900px ){
                  max-width: 100%;
                }
                @media(min-width: 901px ){
                  padding-left: 8.33333%;
                }
                .grid-text-blocks {
                  padding: 50px 0;
                  @media(max-width: 767px) {
                    padding: 32px 0 0px;
                  }
                  h2 {
                    margin-bottom: 24px;
                  }
                  p {
                    margin-bottom: 24px;
                    font-size: 17px;
                    @media(max-width: 767px) {
                      font-size: 16px;
                    }
                  }

                  ul {
                    margin-bottom: 40px;
                    margin-left: 30px;
                    @media(max-width: 767px) {
                      margin-bottom: 30px;
                      margin-left: 0;
                    }
                    li {
                      &:not(:last-child) {
                        margin-bottom: 10px;
                      }
                      font-size: 17px;
                      @media(max-width: 767px) {
                        font-size: 16px;

                      }
                    }
                  }
                }

                @media(max-width: 767px) {
