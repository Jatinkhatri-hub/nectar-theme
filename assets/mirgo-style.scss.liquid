  }
}

/* Animations */
@keyframes slide-up-fade-in{
  0% {
    opacity:0;
    transform:  translate(0px,40px)  ;
  }
  100% {
    opacity:1;
    transform:  translate(0px,0px)  ;
  }
}

@-moz-keyframes slide-up-fade-in{
  0% {
    opacity:0;
    -moz-transform:  translate(0px,40px)  ;
  }
  100% {
    opacity:1;
    -moz-transform:  translate(0px,0px)  ;
  }
}

@-webkit-keyframes slide-up-fade-in {
  0% {
    opacity:0;
    -webkit-transform:  translate(0px,40px)  ;
  }
  100% {
    opacity:1;
    -webkit-transform:  translate(0px,0px)  ;
  }
}

@-o-keyframes slide-up-fade-in {
  0% {
    opacity:0;
    -o-transform:  translate(0px,40px)  ;
  }
  100% {
    opacity:1;
    -o-transform:  translate(0px,0px)  ;
  }
}

@-ms-keyframes slide-up-fade-in {
  0% {
    opacity:0;
    -ms-transform:  translate(0px,40px)  ;
  }
  100% {
    opacity:1;
    -ms-transform:  translate(0px,0px)  ;
  }
}

/* CHANGES CONTENT */

/* HEADER */
div#shopify-section-header {

  background: #fff;
  width: 100%;
  //transition: 1s;
  position: fixed;
  z-index: 99;
  transition: .2s;
  @media(min-width: 768px) {
    &.height_auto {
      height: 71px;
    }
  }
  .main-header-container {
    border-top: 5px solid #294376;
    background: #fff;
    position: relative;
    z-index: 3;
    &:after {
      content: "";
      position: absolute;
      top: 0;
      left: 0;
      width: 100%;
      height: 100%;
      background: #fff;
      z-index: 2;
    }
  }
  .header-additional-atc {
    &.hidden {
      border-bottom: 1px solid rgb(204, 204, 204);
      transform: translateY(-100px);
      transition: transform 320ms ease-in-out 0s;
      opacity: 1;
      visibility: hidden;
      z-index: -1;
      height: 0;
      @media(max-width: 767px) {
        width: 100%;
        position: fixed;
        bottom: -100px;
        transform: unset !important;
        transition: bottom 320ms ease-in-out 0s;
        height: auto;
      }
    }
    @media(max-width: 767px) {
      transform: unset;
      transition: unset;
      position: fixed;
      bottom: 0;
      background: #fff;
    }
    .page-width {
      padding-top: 5.5px;
      padding-bottom: 5.5px;
      @media(max-width: 425px) {
        padding-bottom: 20px;
      }
      .header-row-group {
        display: flex;
        flex-wrap: wrap;
        h2 {
          margin-right: auto;
          font-size: 22px;
          margin-bottom: 0;
          line-height: 22px;
          vertical-align: middle;
          display: flex;
          align-items: center;
          @media(max-width: 767px) {
            display: none;
          }
        }
        button {
          margin-left: auto;
          @media(max-width: 767px) {
            width: 100%;
            height: 50px;
          }
        }
        .haatc_additional_form {
          @media(max-width: 767px) {
            width: 100%;
          }
          .product-form {
            padding-top: 0 !important;
            #rc_container {
              display: none !important;
            }
            .product-form__item {
              margin-bottom: 0;
            }
          }
        }
      }
    }
  }
  &.border-bottom-header {
    .height_auto {
      @media(min-width: 768px) {
        height: 137px;
        transition: .2s;
      }
    }
    .main-header-container {
      border-bottom: 1px solid rgb(204, 204, 204);
    }
    .header-additional-atc {
      &:not(.hidden) {
        transform: translateY(0px) !important;
        transition: transform 320ms ease-in-out 0s;
        z-index: 1;
        opacity: 1 !important;
        visibility: visible;
        border-bottom: 1px solid rgb(204, 204, 204);
        @media(max-width: 767px) {
          //transition: opacity 1000ms ease-out 0s;
          position: fixed;
          bottom: 0;
          background: #fff;
          left: 0;
          width: 100%;
          transition: bottom 320ms ease-in-out 0s;
          border-top: 1px solid #ccc;
          padding-top: 5px;

        }
      }
    }
    @media(min-width: 768px) {
      .site-header {
        .site-header__mobile-nav {
          .grid__item {
            .site-header__logo {
              margin: 20px 0;
              transition: .5s;
            }
            .navigation-additional {
              margin: 20px 0 !important;
              transition: .5s;
            }
          }
        }
      }
    }
    /*
    &:after {
    content: "";
    background: rgb(204, 204, 204);
    width: 100%;
    height: 1px;
    position: absolute;
    bottom: 0;
    left: 0;
    z-index: 999;
  }
    border-bottom: 1px solid rgb(204, 204, 204);
    */
  }
  /*
  &.sticky-header {

  border-top: 0px solid #294376;
}
  */
  .site-header {
    width: 100%;
    margin: 0 auto;
    padding-left: 15px;
    padding-right: 15px;
    border-bottom: unset;
    @media (min-width: 767px) {
      max-width: 700px;
    }
    @media (min-width: 960px) {
      max-width: 934px;
    }
    @media(min-width: 1200px) {
      max-width: 1200px;
    }
    .grid {
      .grid__item {
        .site-header__logo {
          transition: .5s;
          @media(max-width: 767px) {
            padding-left: 0;
            margin: 9px 0;
          }
          a {
            img {
              max-height: 25px;
              object-fit: contain;
              object-position: left;
              @media(max-width: 767px) {
                max-height: 18px;
              }
            }
          }
        }
      }
      .site-header__icons {
        @media(min-width: 768px) {
          display: none;
        }
        @media(max-width: 767px) {
          padding-right: 0;
          display: none;
        }
      }
      #AccessibleNav {
        width: 100%;
        @media(max-width: 767px) {
          display: table-cell !important;
          min-width: 75%;
        }
        .site-nav {
          margin: 15px 0;
          @media(min-width: 768px) {
            text-align: right;
            padding-right: 35px;
          }
          @media(max-width: 767px) {
            padding-right: 30px;
            text-align: right;
            margin: 7px 0;
          }

          li {

            button {
              @media(max-width: 767px) {
                padding:3px 0;
              }
              &:hover {
                color: $hover_main;
                border-bottom: unset;
                .site-nav__label {
                  border-bottom: unset;
                }
              }
              .icon-chevron-down {
                display: none;
              }
            }
            a,
            span {
              text-transform: capitalize;
              font-size: 18px;
              -webkit-font-smoothing: subpixel-antialiased;
              letter-spacing: 1px;
              font-family: $fnt_regular;
              @media(max-width: 767px) {
                font-size: 16px;
              }
              &:hover {
                color: $hover_main;
                border-bottom: unset;
                .site-nav__label {
                  border-bottom: unset;
                }
              }
              &.site-nav__link--active {
                color: $hover_main;
                .site-nav__label {
                  border-bottom: unset;
                }
              }
            }
            .site-nav__dropdown {
              padding: 10px 40px;
              min-width: 280px;
              -webkit-box-shadow: inset 0 5px 5px -5px rgba(0,0,0,0.5);
              -moz-box-shadow: inset 0 5px 5px -5px rgba(0,0,0,0.5);
              box-shadow: inset 0 5px 5px -5px rgba(0,0,0,0.5);
              top: 64px;
              ul {
                li {
                  &:not(:last-child) {
                    border-bottom: 1px solid #142444;
                  }
                  a {
                    padding: 22px 0;
                    line-height: 1;
                    span {
                      font-size: 16px;
                      line-height: 1;
                    }
                  }
                }
              }
            }
          }
        }
        .navigation-additional {
          position: absolute;
          top: 0;
          margin: 20px 0;
          transition: .5s;
          @media(min-width: 768px){
            margin: 30px 0;
            align-items: center;
          }
          right: 0;
          display: flex;
          @media(max-width: 767px) {
            margin: 14px 0;
          }
          a {
            &.site-header__icon {
              &.site-header__account {
                background: #fff;
                border: 2px solid;
              }
              .icon {
                width: 15px;
                height: 15px;
              }
              &.site-header__cart {
                display: none;
              }
            }
            padding: 0;
            display: block;
            width: 100%;
            /* line-height: 34px; */
            width: 24px;
            height: 24px;
            background: #213b70;
            border-radius: 50%;
            display: flex;
            align-items: center;
            justify-content: center;
            font-size: 22px;
            font-weight: 100;
            margin-left: 27px;
            .site-header__cart-count {
              position: unset;
              width: auto;
              height: auto;
              background: transparent;
            }
          }
        }
      }
      /* MOBILE */
      .site-header__icons--plus {
        .site-header__icons-wrapper {
          .site-header__account {
            background: #fff;
            border: 2px solid;
            padding: 0;
            border-radius: 50%;
            display: block;
            line-height: 1;
            width: 21px;
            height: 21px;
            display: flex;
            align-items: center;
            justify-content: center;
            margin: 5px 0px;
            svg {
              width: 15px;
              height: 15px;
            }
          }
          button {
            &.btn--link {
              line-height: 1;
            }
          }
        }
      }
    }
  }
}
