}
.switch-icons {
  position: relative;
  display: inline-block;
  width: 40px;
  height: 24px;
  min-width: 40px;
}

.switch-icons input {
  opacity: 0;
  width: 0;
  height: 0;
}

.switch-icons .slider {
  position: absolute;
  cursor: pointer;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background-color: #ccc;
  -webkit-transition: .4s;
  transition: .4s;
  background-color: #fff;
  border: 2px solid #213B70;
}

.switch-icons .slider:before {
  position: absolute;
  content: "";
  height: 18px;
  width: 18px;
  left: 2px;
  bottom: 1px;
  background-color: #213b70;
  -webkit-transition: .4s;
  transition: .4s;
}

.switch-icons input:checked + .slider {
  background-color: #fff;
}

.switch-icons input:focus + .slider {
  box-shadow: 0 0 1px #fff;
}

.switch-icons input:checked + .slider:before {
  -webkit-transform: translateX(14px);
  -ms-transform: translateX(14px);
  transform: translateX(14px);
}

/* Rounded sliders */
.switch-icons .slider.round {
  border-radius: 34px;
}

.switch-icons .slider.round:before {
  border-radius: 50%;
}

