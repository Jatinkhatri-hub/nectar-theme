.custom-plus {
  position: relative;
  width: 18px;
  height: 18px;
  display: block;

  &:before,
  &:after{
    content: "";
    position: absolute;
    background-color: #88bbf6;
    transition: transform 150ms ease-out;
  }
  &:before{
    top: 0;
    left: 50%;
    width: 2px;
    height: 100%;
    margin-left: -1px;
  }
  &:after{
    top: 50%;
    left: 0;
    width: 100%;
    height: 2px;
    margin-top: -1px;
  }

  &.hover {
    &:hover{
      cursor: pointer;
      &:before{
        transform: rotate(45deg);
      }
      &:after{
        transform: rotate(45deg);
      }
    }
  }
  &.active {
    cursor: pointer;
    &:before{
      transform: rotate(45deg);
    }
    &:after{
      transform: rotate(45deg);
    }
  }
}
